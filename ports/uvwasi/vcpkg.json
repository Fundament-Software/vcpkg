{
  "name": "uvwasi",
  "version": "0.0.14",
  "description": "WASI syscall API built atop libuv",
  "homepage": "https://github.com/nodejs/uvwasi",
  "license": "MIT",
  "dependencies": [
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    },
    {
      "name": "libuv",
      "version>=": "1.44.2"
    }
  ],
  "default-features": [],
  "features": {
    "tests": {
      "description": "Build the unit tests"
    },
    "debug-log": {
      "description": "Enable debug logging"
    },
    "code-coverage": {
      "description": "Enable coverage reporting"
    },
    "asan": {
      "description": "Enable code asan"
    }
  }
}
